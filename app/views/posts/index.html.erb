<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<h1>Posts</h1>
<% @posts.each do |post| %>
<h2><%= post.title %></h2>
<h2><%= post.author %></h2>
<h2><%= post.body %></h2>
<%= link_to 'Show Post',posts_path(post) %>
<%= link_to 'New Post',new_post_path %>
<%= link_to 'Edit',edit_post_path(post) %>
<%= link_to 'Distroy Post',post,method: :delete %>
<% end %>
