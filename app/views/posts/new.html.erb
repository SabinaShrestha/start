<h1>Posts#new</h1>
<p>Find me in app/views/posts/new.html.erb</p>
<h2>New Posts</h2>

<%= form_for @posts do |f| %>

  <%= f.label :title %>:
  <%= f.text_field :title %>

  <%= f.label :author %>:
  <%= f.text_field :author %>

  <%= f.label :body %>:
  <%= f.text_field :body%>

  <%= f.submit %>

<% end %>
